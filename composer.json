{
    "name": "yoeunes/toastr",
    "description": "Easy toastr notifications for Laravel 5",
    "keywords": ["toastr", "notification", "laravel", "php"],
    "homepage": "https://github.com/yoeunes/toastr",
    "license": "MIT",
    "authors": [
        {
            "name": "Younes Khouza",
            "email": "younes.khoubza@gmail.com",
            "homepage": "https://dalilbook.com",
            "role"    : "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.5",
        "illuminate/session": "^5.5"
    },
    "autoload": {
        "psr-0": {
            "Yoeunes\\Toastr\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Yoeunes\\Toastr\\ToastrServiceProvider"
            ],
            "aliases": {
                "Toastr": "Yoeunes\\Toastr\\Facades\\Toastr"
            }
        }
    }
}
